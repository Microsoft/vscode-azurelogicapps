{
    "compilerOptions": {
        "esModuleInterop": true,
        "lib": [
            "dom",
            "es2015",
            "es2016",
            "es2017"
        ],
        "module": "amd",
        "moduleResolution": "node",
        "noFallthroughCasesInSwitch": true,
        "noImplicitReturns": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "removeComments": true,
        "sourceMap": false,
        "strict": true,
        "target": "es5"
    }
}